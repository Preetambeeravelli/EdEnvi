ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;  
  text-align: right;
}

li {
  float: right;
 
}

li a {
  display: block;
  color: white;
  text-align:center;
  padding: 14px 16px;
  text-decoration: none;
}

li a:hover {
  background-color: #111;
}
.listItems{
	color: white;
	float: left;
}
.input {
  padding: 12px 20px;
  font-size: 15px;
  border-width: 2px;
  border-radius: 18px;
  border-style: solid;
  font-family: sans-serif;
  width: 60%;
  box-sizing: border-box;
  margin: 8px 0;

}
.button {
  border-radius: 28px;
  font-family: Georgia;
  color: white;
  font-size: 20px;
  background: #4c7397;
  padding: 10px 20px 10px 20px;
  text-decoration: none;
}

.button:hover {
  background: #425465;
  text-decoration: none;
}
.footer{
  position: relative;
  text-align: center;
  color: white;
}

/* Bottom left text */
.bottom-left {
  position: absolute;
  bottom: 8px;
  left: 16px;
}

/* Top left text */
.top-left {
  position: absolute;
  top: 20px;
  left: 16px;
}

/* Top right text */
.top-right {
  position: absolute;
  top: 8px;
  right: 16px;
}

/* Bottom right text */
.bottom-right {
  position: absolute;
  bottom: 8px;
  right: 16px;
}

/* Centered text */
.centered {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

